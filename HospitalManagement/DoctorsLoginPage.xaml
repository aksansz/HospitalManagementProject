<Window x:Class="HospitalManagement.DoctorsLoginPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HospitalManagement"
        mc:Ignorable="d"
        Title="DoctorsLoginPage" Height="450" Width="800">
    <Grid Margin="190,0,190,0" Height="201">
        <TextBox Margin="150,49,150,132" Text="Username" TextWrapping="Wrap" Width="120" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <TextBox Margin="150,87,150,93" Text="Password" TextWrapping="Wrap" Width="120" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Button Content="Login" Margin="0,115,0,29" HorizontalAlignment="Center" VerticalAlignment="Center"/>

    </Grid>
</Window>
