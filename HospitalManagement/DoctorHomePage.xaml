<Window x:Class="HospitalManagement.DoctorHomePage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HospitalManagement"
        mc:Ignorable="d"
        Title="DoctorHomePage" Height="450" Width="800">
    <Grid Height="376" RenderTransformOrigin="0.501,0.473">
        <Grid x:Name="grid_doctorPage" Margin="26,97,21,87">
            <DataGrid x:Name="data_grid_doctorpage_appointmentList" SelectedCellsChanged="data_grid_doctorpage_appointmentList_SelectedCellsChanged" Margin="0,0,0,10"/>
        </Grid>
        <Label x:Name="label_doctor_welcome_message" Content="dfdsfdsfdsfdsf" Margin="0,29,0,249" HorizontalAlignment="Center" FontWeight="Normal" FontSize="20" RenderTransformOrigin="0.464,-0.482" Width="275" VerticalAlignment="Center"/>
        <Button x:Name="btn_update_doctor_notes" Content="Update Doctor Notes" HorizontalAlignment="Center" Height="32" Margin="0,314,0,0" VerticalAlignment="Bottom" Width="138" Click="btn_update_doctor_notes_click"/>
        <TextBox x:Name="textbox_doctor_notes" HorizontalAlignment="Left" Margin="26,289,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="753" Height="50"/>

    </Grid>
</Window>
